  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="http://www.semanticweb.org/ontologies/erowlmapper#isAttributeAOf">
    <owl:inverseOf>
      <owl:InverseFunctionalProperty rdf:about="http://www.semanticweb.org/ontologies/erowlmapper#hasAttributeA"/>
    </owl:inverseOf>
    <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/erowlmapper#EntityA"/>
    <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/erowlmapper#AttributeA"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
  </owl:FunctionalProperty>
  <owl:InverseFunctionalProperty rdf:about="http://www.semanticweb.org/ontologies/erowlmapper#hasAttributeA">
    <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/erowlmapper#AttributeA"/>
    <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/erowlmapper#EntityA"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:InverseFunctionalProperty>
