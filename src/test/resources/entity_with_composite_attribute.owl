<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <owl:Class rdf:about="http://www.semanticweb.org/ontologies/erowlmapper#EntityA"/>
  <owl:Class rdf:about="http://www.semanticweb.org/ontologies/erowlmapper#AttributeA">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:minCardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="http://www.semanticweb.org/ontologies/erowlmapper#isAttributeAOf"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:FunctionalProperty rdf:about="http://www.semanticweb.org/ontologies/erowlmapper#hasAttributeB">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/erowlmapper#AttributeA"/>
